<h1> Community </h1>

<% users.forEach(function(user) {%>
<section class="profile-picture-box">
    <img class="profile-pic" src="<%= user.photo %>">
</section>
    <section class="one-user">
        <p> <%= user.name %> </p>
        <p> <%= user.location %> </p>
        <p> <%= user.email %> </p>
        <a href="/messages"><button id="send-msg"> Send a Message </button></a>
    </section>

<%})%>